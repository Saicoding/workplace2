/* pages/tiku/modelReal/modelRealList/modelRealList.wxss */
.item{
  width:100%;
  height:120rpx;
  border-bottom: 1px solid #eeeeee;
  padding-left:25rpx;
}

.loading-img{
  width: 750rpx;
  height:458rpx;
  margin:0 auto;
}

.circle{
  display: float;
  float:left;
  width:35rpx;
  height:35rpx;
  border-radius: 35rpx;
  margin-top:46rpx;
}

.title{
  display: float;
  float:left;
  font-size:35rpx;
  width:73%;
  margin-top:40rpx;
  margin-left:30rpx;
  white-space: nowrap;
  overflow: hidden;
}

.num-img-container{
  display: float;
  float:right;
  margin-top:40rpx;
  margin-left:30rpx;
}

.num-img{
  width:40rpx;
  height:40rpx;
}

.score{
  display: float;
  float:left;
  margin-top:35rpx;
  margin-left:30rpx;
}

.socre-text{
  font-size:35rpx;
}

.score-img{
  width:60rpx;
  height:60rpx;
}

.select{
  background: rgb(223, 223, 223);
}