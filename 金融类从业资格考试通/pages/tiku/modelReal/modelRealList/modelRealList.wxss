/* pages/tiku/modelReal/modelRealList/modelRealList.wxss */

.item {
  width: 100%;
  height: 120rpx;
  border-bottom: 1px dashed #eee;
  padding-left: 25rpx;
}

.loading-img {
  width: 750rpx;
  height: 458rpx;
  margin: 0 auto;
}

.circle {
  display: float;
  float: left;
  width: 35rpx;
  height: 35rpx;
  border-radius: 35rpx;
  margin-top: 46rpx;
}

.title {
  display: float;
  float: left;
  font-size: 35rpx;
  width: 73%;
  margin-top: 40rpx;
  margin-left: 30rpx;
  white-space: nowrap;
  overflow: hidden;
}

.score {
  display: float;
  float: left;
  width: 50rpx;
  height: 50rpx;
  line-height: 47rpx;
  margin-top: 40rpx;
  margin-left: 30rpx;
  font-size: 25rpx;
  text-align: center;
}

.select {
  background: rgb(223, 223, 223);
}
