/* pages/mine/set/set.wxss */

page {
  background: #f9f9f9;
}

.mycontainer {
  background: #f9f9f9;
  width: 100%;
  margin-top: 18rpx;
  height: 300rpx;
}

.block {
  height: 80rpx;
  width: 100%;
  background: white;
  border-bottom: 1rpx solid #ebebeb;
  line-height: 80rpx;
  font-size: 30rpx;
  padding-left: 40rpx;
}

.right-img, .mytext {
  display: float;
  float: left;
}

.mytext {
  width: 660rpx;
  height: 80rpx;
  line-height: 80rpx;
}

.right-img {
  width: 30rpx;
  height: 30rpx;
  margin-top: 25rpx;
}

.select {
  background: rgb(230, 229, 229);
}

.picker {
  width: 680rpx;
  height: 80rpx;
  line-height: 80rpx;
}

.mulu, .picker-text {
  display: float;
}

.mulu {
  float: left;
  text-align: left;
  width:580rpx;
}

.piker-text {
  float: right;
}
