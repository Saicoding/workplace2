/* components/errorRecovery/errorRecovery.wxss */

.container-out{
  width:90%;
  border:1px solid#f3f3f3;
  margin: 0 auto;
  z-index: 100;
  background: white;
}
.container-in{
  width:90%;
  position: absolute;
  background: white;
  z-index: 100;
  border-radius:15rpx;
  padding-bottom:40rpx;
}


.modal{
  position: absolute;
  top:0rpx;
  left:0rpx;
  width:100%;
  height:100%;
  filter:Alpha(Opacity=60);
  background-color:#1F1F1F;
  opacity: 0.5;
  z-index: 99;
}

.title1,.title2{
  width:90%;
  margin:0 auto;
  font-size:42rpx;
  text-align: center;
}

.title1{
  margin-top:30rpx;
  margin-bottom:20rpx;
}

.title2{
  height:80rpx;
  font-weight:bold;
  margin-top:20rpx;
  line-height: 30rpx;
}

.detail-text-title{
  width:300rpx;
  height:55rpx;
  border-radius: 50rpx;
  background: #0197f6;
  color:white;
  font-size:35rpx;
  text-align: center;
  margin-top:40rpx;
  margin-left:35rpx;
}

.detail-block{
  width:100%;
  height:70rpx;
  color:#7e7e7e;
}

.detail-icon,.detail-text{
  display: float;
  float:left;
}

.detail-icon{
  width:20rpx;
  height:20rpx;
  border-radius:20rpx;
  background: #79e25e;
  margin-top:25rpx;
  margin-left:70rpx;
}

.detail-text{
  height:70rpx;
  line-height: 70rpx;
  font-size:32rpx;
  margin-left:5rpx;
}

.bottom{
  width:100%;
  height:90rpx;
  line-height: 90rpx;
    margin-top:40rpx;
}

.bottom-text,.submit{
  display: float;
  float:left;
}

.bottom-text{
  font-size:25rpx;
  letter-spacing: 4rpx;
  margin-left:40rpx;
}

.submit{
  width:310rpx;
  height:90rpx;
  margin-left:40rpx;
}

.content{
  border-radius:5rpx;
  font-size:30rpx;
  width:100%;
  line-height: 50rpx;
  margin:0rpx auto;
  border-bottom:1rpx solid #d5d5d5;
  border-top:1rpx solid #d5d5d5;
  
  background-image: url("data:image/png;base64,") ;
  background-repeat:no-repeat; 
  background-size:100% 65%;
  background-position:bottom;
}
